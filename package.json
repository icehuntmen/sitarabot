{
  "name": "unitebot",
  "version": "1.0.0",
  "description": "Unitebot",
  "main": "server.js",
  "scripts": {
    "docs": "jsdoc ./ -r -c ./src-docs/jsdoc.json && auto-changelog",
    "foodocs": "jsdoc ./ -r -c ./src-docs/foodoc.json && auto-changelog",
    "pm2start": "pm2 start appbot.js --watch",
    "pm2rest": "pm2 restart appbot.js",
    "pm2stop": "pm2 stop appbot.js",
    "test": "NODE_ENV=development&&nodemon testdrive.js",
    "start": "NODE_ENV=development&&nodemon server.js",
    "server": "NODE_ENV=development&&nodemon  --trace-warnings appbot.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "mern",
    "react"
  ],
  "author": [
    "Cold Roman <colddevelop@gmail.com>",
    "Alexander Hunter <icehuntmen@gmail.com>"
  ],
  "license": "ISC",
  "auto-changelog": {
    "output": "HISTORY.md",
    "template": "changelog.hbs",
    "unreleased": true,
    "commitLimit": false
  },
  "dependencies": {
    "@danke77/google-translate-api": "^1.0.0",
    "@discordjs/opus": "^0.3.2",
    "assert": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "buffer": "^5.6.0",
    "canvas": "^2.6.1",
    "canvas-constructor": "^4.1.0",
    "child_process": "^1.0.2",
    "common-tags": "^1.8.0",
    "config": "^3.3.2",
    "console-table-printer": "^2.4.17",
    "create-react-app": "^3.4.1",
    "cron": "^1.8.2",
    "discord-canvas": "^1.3.2",
    "discord.js": "^12.3.1",
    "discord.js-collector": "^1.5.5",
    "discord.js-form": "^1.0.4",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "foodoc": "0.0.9",
    "i18n": "^0.10.0",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.0.3",
    "log4js": "^6.3.0",
    "log4js-json-layout": "^2.2.2",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.31",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.18",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "node-notifier": "^8.0.0",
    "node-schedule": "^1.3.2",
    "ora": "^5.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "shortid": "^2.2.15",
    "simple-git": "^2.21.0",
    "slash-commands": "^1.3.1",
    "slash-create": "^2.1.0",
    "spinnies": "^0.5.1",
    "table": "^5.4.6",
    "translate": "^0.7.0"
  },
  "devDependencies": {
    "auto-changelog": "^2.2.1",
    "better-docs": "^2.3.2",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "countly-docdash": "^0.5.0",
    "cross-env": "^7.0.2",
    "docdash": "git+https://github.com/clenemt/docdash.git",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.6",
    "jsdoc-mermaid": "^1.0.0",
    "node-typewriter": "0.0.2",
    "nodemon": "^2.0.4"
  }
}
