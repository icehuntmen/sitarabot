{
  "tags": {
    "allowUnknownTags":true

  },
  "source": {
    "include": ["package.json", "README.md"],
    "exclude": [ "node_modules","docs","src-docs"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|src-docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "templates": {
    "useLongnameInNav": true,
    "showInheritedInNav": true,
    "systemName"            : "UNITEBOT",
    "systemSummary"         : "「CODΞ」DΞVS",
    "systemLogo"            : "",
    "systemColor"           : "",
    "footer"                : "Redmine <a href='https://unitebot.fun'>https://unitebot.fun</a>",
    "copyright"             : "Copyright by「CODΞ」DΞVS 2020",
    "includeDate"           : "true",
    "dateFormat"            : "dddd, MMMM Do YYYY, h:mm:ss a",
    "inlineNav"             : false,
    "inverseNav"            : true,
    "navMembers"            : [
      {"kind": "class", "title": "Classes", "summary": "All documented classes."},
      {"kind": "external", "title": "Externals", "summary": "All documented external members."},
      {"kind": "global", "title": "Globals", "summary": "All documented globals."},
      {"kind": "mixin", "title": "Mixins", "summary": "All documented mixins."},
      {"kind": "interface", "title": "Interfaces", "summary": "All documented interfaces."},
      {"kind": "module", "title": "Modules", "summary": "All documented modules."},
      {"kind": "namespace", "title": "Namespaces", "summary": "All documented namespaces."},
      {"kind": "tutorial", "title": "Tutorials", "summary": "All available tutorials."}
    ],
    "linenums"              : true,
    "showTableOfContents"   : true,
    "showAccessFilter"      : true,
    "analytics"             : "",
    "collapseSymbols"       : true,
    "methodHeadingReturns"  : true,
    "outputSourceFiles"     : true,
    "outputSourcePath"      : true,
    "sort"                  : "since",
    "search"                : true,
    "favicon"               : "",
    "stylesheets"           : [],
    "scripts"               : []
  },
  "opts": {
    "destination": "./docs/unitebot/1.0.0",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./src-docs/foodoc/template",
    "tutorials": "./src-docs/tutorials/"
  }
}